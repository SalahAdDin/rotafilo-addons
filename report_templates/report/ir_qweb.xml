<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="contact_inherit_name" inherit_id="base.contact_name">
        <xpath expr="//span[@t-esc='name']" position="replace">
            <strong>
                <span itemprop="name" t-esc="name"/>
            </strong>
        </xpath>
    </template>
</odoo>
